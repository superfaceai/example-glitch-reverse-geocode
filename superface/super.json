{
  "profiles": {
    "address/geocoding": {
      "version": "3.0.1",
      "providers": {
        "nominatim": {
          "file": "./maps/geocode.nominatim.suma",
          "retryPolicy": "circuit-breaker"
        },
        "azure-maps": {
          "file": "./maps/geocode.azure-maps.suma",
          "retryPolicy": "none",
          "defaults": {
            "ReverseGeocode": {
              "input": {
                "subscriptionKey": "$SUBSCRIPTION_KEY"
              }
            }
          }
        },
        "google-apis": {},
        "opencage": {},
        "tomtom": {
          "file": "./maps/geocode.tomtom.suma"
        },
