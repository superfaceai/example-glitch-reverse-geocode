        },
        "yandex": {
          "file": "./maps/geocode.yandex.suma"
        },
        "yahoo": {
          "file": "./maps/geocode.yahoo.suma"
        },
        "mapquest": {
          "file": "./maps/geocode.mapquest.suma"
        },
        "yelp": {
          "file": "./maps/geocode.yelp.suma"
        }
      }
    }
  },
  "providers": {
    "mock": {
      "security": []
    },
